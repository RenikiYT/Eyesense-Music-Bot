{
  "name": "eyesense-music-bot",
  "version": "11.0.0",
  "description": "Bot to play Music from Youtube, Soundcloud or Spotify using the new slash commands and a lot more!",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "start": "node dist/index.js",
    "dev": "run-s build start",
    "watch:start": "tsc-watch --onSuccess \"node ./dist/index.js\"",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "prettier": "@sapphire/prettier-config",
  "keywords": [
    "music",
    "discord-music",
    "discord-music-bot",
    "music-discord-bot",
    "eyesense",
    "spotify",
    "soundcloud",
    "bot",
    "yotutbe-bot",
    "discord-bot"
  ],
  "author": "naseif",
  "license": "ISC",
  "dependencies": {
    "@discordjs/builders": "^0.12.0",
    "@discordjs/collection": "^0.6.0",
    "@discordjs/opus": "^0.7.0",
    "@discordjs/rest": "^0.3.0",
    "@discordjs/voice": "^0.8.0",
    "@lavaclient/queue": "^2.0.6",
    "@lavaclient/spotify": "^3.0.0",
    "@sapphire/decorators": "^4.3.4",
    "@sapphire/discord-utilities": "^2.10.3",
    "@sapphire/discord.js-utilities": "^4.9.7",
    "@sapphire/fetch": "^2.3.1",
    "@sapphire/framework": "^2.4.1",
    "@sapphire/plugin-api": "^3.2.1",
    "@sapphire/plugin-editable-commands": "^1.2.0",
    "@sapphire/plugin-logger": "^2.2.1",
    "@sapphire/plugin-subcommands": "^2.2.0",
    "@sapphire/time-utilities": "^1.7.4",
    "@sapphire/type": "^2.2.0",
    "@sapphire/utilities": "^3.6.2",
    "bufferutil": "^4.0.6",
    "devtools-ts": "^1.1.0",
    "discord-api-types": "^0.27.3",
    "discord-player": "^5.2.2",
    "discord.js": "^13.6.0",
    "ffmpeg-static": "^4.4.1",
    "genius-lyrics-api": "^3.2.0",
    "jikanjs": "^0.7.0",
    "lavaclient": "^4.0.8",
